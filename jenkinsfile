pipeline {
    agent any
    // tools {
        //nodejs "nodejs"
    //}
    stages {
        stage("intall") {
            steps {
                sh 'npm install'
            }
        }
        stage("build") {
            steps {
                sh 'npm run build'
            }
        }
    }
    post {
        success {
            echo "SUCCESS"
        }
        failure {
            echo "FAILURE"
        }
    }
}